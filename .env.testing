ENVIRONMENT=testing
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-5-mini
OPENAI_MAX_TOKENS=4096
OPENAI_TEMPERATURE=1
LOG_LEVEL=INFO
# PostgreSQL test database (required)
DATABASE_URL=postgresql://telegram_bot:telegram_bot_password@localhost:5432/telegram_bot_test

# Qdrant Vector Database Configuration (testing uses same local Qdrant)
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=
QDRANT_COLLECTION_NAME=football_documents_test

# Embedding Configuration
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_BATCH_SIZE=100
TOP_K_RETRIEVALS=5
SIMILARITY_THRESHOLD=0.7
